@model List<Book>
<form action="/book/search">
    <input type="search" name="q"></input>
    <button>Search</button>
</form>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>
                    <img src="@item.ImageUrl" @item.Title width="150"></img>
                </td>
            </tr>
        } 
    </tbody>
</table>  