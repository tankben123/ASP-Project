@model List<IdentityRole>
<a class="btn btn-primary" href="/dashboard/role/add">Add role</a>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Command</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>   
                <td>@role.Name</td>
                <td>
                    <a class="btn btn-primary" href="/dashboard/role/edit/@role.Id">Edit</a>
                    <a class="btn btn-primary" href="/dashboard/role/delete/@role.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>